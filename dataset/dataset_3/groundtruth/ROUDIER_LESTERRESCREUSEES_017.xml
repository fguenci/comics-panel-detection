    <svg contentScriptType="text/ecmascript" zoomAndPan="magnify" contentStyleType="text/css" class="Line" preserveAspectRatio="xMidYMid meet" version="1.0">
        <polygon points="787,47 1134,47 1134,91 787,91 787,47" id="ROUDIER_LESTERRESCREUSEES_017_line_0">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_0" textType="speech">Croyez-en mon expé-</metadata>
        </polygon>
        <polygon points="759,88 1164,88 1164,131 759,131 759,88" id="ROUDIER_LESTERRESCREUSEES_017_line_1">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_0" textType="speech">rience, il va en chier votre</metadata>
        </polygon>
        <polygon points="801,131 1122,131 1122,165 801,165 801,131" id="ROUDIER_LESTERRESCREUSEES_017_line_2">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_0" textType="speech">homme à tout faire</metadata>
        </polygon>
        <polygon points="855,329 1075,329 1075,370 855,370 855,329" id="ROUDIER_LESTERRESCREUSEES_017_line_3">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_1" textType="speech">Il est costaud,</metadata>
        </polygon>
        <polygon points="853,369 1077,369 1077,412 853,412 853,369" id="ROUDIER_LESTERRESCREUSEES_017_line_4">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_1" textType="speech">mais n'est pas</metadata>
        </polygon>
        <polygon points="841,410 1088,410 1088,455 841,455 841,410" id="ROUDIER_LESTERRESCREUSEES_017_line_5">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_1" textType="speech">videur qui veut.</metadata>
        </polygon>
        <polygon points="1185,341 1531,341 1531,381 1185,381 1185,341" id="ROUDIER_LESTERRESCREUSEES_017_line_6">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_2" textType="speech">Il faut être du métier,</metadata>
        </polygon>
        <polygon points="1158,381 1559,381 1559,432 1158,432 1158,381" id="ROUDIER_LESTERRESCREUSEES_017_line_7">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_2" textType="speech">moi je suis du métier, lui,</metadata>
        </polygon>
        <polygon points="1177,423 1543,423 1543,472 1177,472 1177,423" id="ROUDIER_LESTERRESCREUSEES_017_line_8">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_2" textType="speech">non. Regardez-le, il ne</metadata>
        </polygon>
        <polygon points="1177,468 1546,468 1546,514 1177,514 1177,468" id="ROUDIER_LESTERRESCREUSEES_017_line_9">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_2" textType="speech">tiendra jamais le coup.</metadata>
        </polygon>
        <polygon points="1437,552 1740,552 1740,597 1437,597 1437,552" id="ROUDIER_LESTERRESCREUSEES_017_line_10">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_3" textType="speech">Vou verrez, quand</metadata>
        </polygon>
        <polygon points="1409,596 1772,596 1772,638 1409,638 1409,596" id="ROUDIER_LESTERRESCREUSEES_017_line_11">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_3" textType="speech">il n'en pourra plus vous</metadata>
        </polygon>
        <polygon points="1427,636 1754,636 1754,678 1427,678 1427,636" id="ROUDIER_LESTERRESCREUSEES_017_line_12">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_3" textType="speech">ne viendrez pas vou</metadata>
        </polygon>
        <polygon points="1411,676 1769,676 1769,720 1411,720 1411,676" id="ROUDIER_LESTERRESCREUSEES_017_line_13">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_3" textType="speech">plaindre. Je vous aurai</metadata>
        </polygon>
        <polygon points="1526,720 1658,720 1658,760 1526,760 1526,720" id="ROUDIER_LESTERRESCREUSEES_017_line_14">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_3" textType="speech">prévenu.</metadata>
        </polygon>
        <polygon points="935,621 1084,621 1084,657 935,657 935,621" id="ROUDIER_LESTERRESCREUSEES_017_line_15">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_4" textType="speech">Pour faire</metadata>
        </polygon>
        <polygon points="835,674 1184,674 1184,716 835,716 835,674" id="ROUDIER_LESTERRESCREUSEES_017_line_16">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_4" textType="speech">correctement un trou,</metadata>
        </polygon>
        <polygon points="877,715 1139,715 1139,752 877,752 877,715" id="ROUDIER_LESTERRESCREUSEES_017_line_17">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_4" textType="speech">il faut être deux.</metadata>
        </polygon>
        <polygon points="763,1086 870,1086 870,1129 763,1129 763,1086" id="ROUDIER_LESTERRESCREUSEES_017_line_18">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_5" textType="speech">Un qui</metadata>
        </polygon>
        <polygon points="716,1134 919,1134 919,1164 716,1164 716,1134" id="ROUDIER_LESTERRESCREUSEES_017_line_19">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_5" textType="speech">creuse et un</metadata>
        </polygon>
        <polygon points="750,1173 885,1173 885,1219 750,1219 750,1173" id="ROUDIER_LESTERRESCREUSEES_017_line_20">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_5" textType="speech">qui vide.</metadata>
        </polygon>
        <polygon points="1089,1494 1451,1494 1451,1536 1089,1536 1089,1494" id="ROUDIER_LESTERRESCREUSEES_017_line_21">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">Et un bon videur, c'est</metadata>
        </polygon>
        <polygon points="1054,1537 1485,1537 1485,1583 1054,1583 1054,1537" id="ROUDIER_LESTERRESCREUSEES_017_line_22">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">quelqu'un qui sait soulever</metadata>
        </polygon>
        <polygon points="1089,1580 1450,1580 1450,1625 1089,1625 1089,1580" id="ROUDIER_LESTERRESCREUSEES_017_line_23">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">la terre aussi bien que</metadata>
        </polygon>
        <polygon points="1130,1626 1411,1626 1411,1672 1130,1672 1130,1626" id="ROUDIER_LESTERRESCREUSEES_017_line_24">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">je sais la creuser.</metadata>
        </polygon>
        <polygon points="1344,1704 1728,1704 1728,1743 1344,1743 1344,1704" id="ROUDIER_LESTERRESCREUSEES_017_line_25">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">SI votre demoiselle nous</metadata>
        </polygon>
        <polygon points="1295,1748 1778,1748 1778,1793 1295,1793 1295,1748" id="ROUDIER_LESTERRESCREUSEES_017_line_26">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">claque entre les doigts, on est</metadata>
        </polygon>
        <polygon points="1296,1792 1777,1792 1777,1840 1296,1840 1296,1792" id="ROUDIER_LESTERRESCREUSEES_017_line_27">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">bon pour rentrer à la maison.</metadata>
        </polygon>
        <polygon points="1299,1836 1777,1836 1777,1881 1299,1881 1299,1836" id="ROUDIER_LESTERRESCREUSEES_017_line_28">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">COmme c'est parti, au mieux,</metadata>
        </polygon>
        <polygon points="1393,1880 1683,1880 1683,1914 1393,1914 1393,1880" id="ROUDIER_LESTERRESCREUSEES_017_line_29">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_6" textType="speech">il va nous ralentir.</metadata>
        </polygon>
        <polygon points="337,2483 631,2483 631,2533 337,2533 337,2483" id="ROUDIER_LESTERRESCREUSEES_017_line_30">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_7" textType="speech">C'est ce que je dis</metadata>
        </polygon>
        <polygon points="337,2530 635,2530 635,2576 337,2576 337,2530" id="ROUDIER_LESTERRESCREUSEES_017_line_31">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_7" textType="speech">Lecreux, vous êtes</metadata>
        </polygon>
        <polygon points="341,2574 634,2574 634,2619 341,2619 341,2574" id="ROUDIER_LESTERRESCREUSEES_017_line_32">
            <metadata method="" typography="" idBalloon="ROUDIER_LESTERRESCREUSEES_017_balloon_7" textType="speech">un homme pressé.</metadata>
        </polygon>
